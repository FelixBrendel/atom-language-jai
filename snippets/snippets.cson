'.source.odin':
  'println':
    'prefix': 'pl'
    'body': 'println("$1");$2'
  'proc':
    'prefix': 'pr'
    'body': '$1 :: proc ($2) ${3:-> }{\n\t$4\n}'
  'struct':
    'prefix': 'st'
    'body': '$1 :: struct {\n\t$2\n}'
  'enum':
    'prefix': 'en'
    'body': '$1 :: enum {\n\t$2\n}'

  'if':
    'prefix': 'if'
    'body': 'if $1 {\n\t$2\n}'
  'if else':
    'prefix': 'ife'
    'body': 'if $1 {\n\t$2\n} else {\n\t$3\n}'
